flowchart LR
    classDef in fill:#E3F2FD,stroke:#2196F3,stroke-width:2px;
    classDef para fill:#FFF9C4,stroke:#FBC02D,stroke-width:2px;
    classDef act fill:#E1BEE7,stroke:#9C27B0,stroke-width:2px;
    classDef func fill:#F8BBD0,stroke:#E91E63,stroke-width:2px;
    classDef out fill:#DCEDC8,stroke:#8BC34A,stroke-width:2px;
    classDef skip stroke:#B0BEC5,stroke-width:2px,stroke-dasharray: 5 5;

    X1((x<sub>1</sub>)):::in
    X2((x<sub>2</sub>)):::in
    H1["H<sub>1</sub>"]:::para
    H2["H<sub>2</sub>"]:::para
    H3["H<sub>3</sub>"]:::para
    H4["H<sub>4</sub>"]:::para
    M1((m<sub>1</sub>)):::act
    M2((m<sub>2</sub>)):::act
    M3((m<sub>3</sub>)):::act
    M4((m<sub>4</sub>)):::act
    O["O"]:::para
    Y((y)):::out

    X1 --> H1 --> M1 --> O --> Y
    X1 --> H2 --> M2 --> O
    X1 --> H3 --> M3 --> O
    X1 --> H4 --> M4 --> O
    X2 --> H1
    X2 --> H2
    X2 --> H3
    X2 --> H4
